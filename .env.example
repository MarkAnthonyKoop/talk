# Example environment variables for Talk framework
# Copy this to .env and add your API keys

# Google Gemini
GEMINI_API_KEY=your-gemini-api-key-here

# OpenAI
OPENAI_API_KEY=your-openai-api-key-here

# Anthropic Claude
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Perplexity
PERPLEXITY_API_KEY=your-perplexity-api-key-here

# Fireworks
FIREWORKS_API_KEY=your-fireworks-api-key-here

# OpenRouter
OPENROUTER_API_KEY=your-openrouter-api-key-here

# Talk Framework Settings
TALK_LLM_PROVIDER=google
TALK_FORCE_MODEL=gemini-2.0-flash
TALK_LOG_LEVEL=INFO